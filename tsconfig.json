{
  "compilerOptions": {
    "target": "ES2018",
    "lib": ["ES2018"],
    "module": "CommonJS",
    "outDir": "./",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "types": ["@figma/plugin-typings"]
  },
  "include": ["./code.ts"],
  "exclude": ["node_modules"]
}